{
  "name": "sfeirschoolterraform",
  "version": "1.0.0",
  "description": "Sfeir School Terraform with revealJS",
  "scripts": {
    "sass": "sass ./scss/slides.scss:./css/slides.css --source-map --watch",
    "sass-once": "sass ./scss/slides.scss:./css/slides.css --source-map",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "live-server --port=4242 --open=\"./index.html\" --watch=\"markdown,scripts/\"",
    "start": "parallelshell \"npm run serve\" \"npm run sass\"",
    "prepare": "node ./scripts/dont-touch/prepare-script.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sfeir-open-source/sfeir-school-terraform.git"
  },
  "keywords": [
    "terraform",
    "sfeir"
  ],
  "author": "jnahelou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sfeir-open-source/sfeir-school-terraform/issues"
  },
  "homepage": "https://github.com/sfeir-open-source/sfeir-school-terraform",
  "dependencies": {
    "sfeir-school-theme": "^3.1.0"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "live-server": "^1.2.1",
    "parallelshell": "3.0.1",
    "prettier": "^3.2.5", 
    "sass": "^1.72.0",
    "shelljs": "^0.8.5"
  },
  "snowpack": {
    "webDependencies": [
      "sfeir-school-theme/dist/**/*",
      "lit-html",
      "reveal.js/**/^(?!LICENSE)(.*)",
      "highlight.js/style/**/*"
    ]
  }
}
